@model MoveViewModel

<h1>List of Moves</h1>

<ul>
    @foreach (var tuple in Model.MoveWithAverageRatingTuples)
    {
        <li>@tuple.move.Name - @Math.Round(tuple.avg, 2) </li>
    }
</ul>
